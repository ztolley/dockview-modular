{
  "name": "@ztolley/hello-panel",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/hello-panel.js",
  "module": "./dist/hello-panel.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hello-panel.js",
      "default": "./dist/hello-panel.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "npm run clean && npm run build:types && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "dockview-core": "^4.7.1"
  },
  "devDependencies": {
    "dockview-core": "^4.7.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.0",
    "vite": "^7.1.6"
  },
  "dockview": {
    "panels": [
      {
        "name": "greetingPanel",
        "module": "./dist/hello-panel.js",
        "export": "GreetingPanel",
        "title": "Greeting Panel"
      }
    ]
  }
}
